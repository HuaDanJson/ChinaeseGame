<?xml version="1.0" encoding="utf-8"?>
<manifest package="cool.chinalanguagegame.android"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".StoreActivity">
        </activity>
        <activity android:name=".SelectGameActivity">
        </activity>
        <activity android:name=".PlayGameActivity">
        </activity>
        <activity android:name=".GameOverActivity">
        </activity>
        <activity android:name=".SmallChiActivity">
        </activity>
        <activity android:name=".MiddleChengYuActivity">
        </activity>
        <activity android:name=".MiddleChiActivity">
        </activity>
    </application>

</manifest>